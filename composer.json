{
    "name": "gemorroj/archive7z",
    "description": "7z cli wrapper",
    "keywords": ["7z","archive","p7zip","7-zip","7zip"],
    "type": "library",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/process": "^3.3|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Archive7z\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Archive7z\\Tests\\": "tests/"
        }
    }
}
